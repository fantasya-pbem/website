{
	"name": "fantasya-pbem/website",
	"description": "The Fantasya website.",
	"keywords": ["atlantis", "email", "eressea", "fantasya", "lemuria", "pbem", "symfony", "website"],
	"license": "proprietary",
	"type": "project",
	"homepage": "https://www.fantasya-pbem.de",
	"support": {
		"chat": "https://web.libera.chat/?channels=#fantasya",
		"email": "info@fantasya-pbem.de",
		"forum": "https://forum.fantasya-pbem.de",
		"irc": "irc://libera.chat/fantasya",
		"issues": "https://github.com/fantasya-pbem/website/issues",
		"source": "https://github.com/fantasya-pbem/website",
		"wiki": "https://wiki.fantasya-pbem.de"
	},
	"funding": [
		{
			"type": "liberapay",
			"url": "https://liberapay.com/Fantasya"
		},
		{
			"type": "other",
			"url": "https://www.fantasya-pbem.de/donate"
		}
	],
	"authors": [
		{
			"name": "Sascha Ternes",
			"email": "info@fantasya-pbem.de"
		}
	],
	"version": "2.5.3",
	"time": "2021-12-01",

	"require": {
		"php": "~8.0",
		"ext-ctype": "*",
		"ext-iconv": "*",
		"ext-pdo": "*",
		"components/jquery": "~3.6",
		"doctrine/annotations": "~1.13",
		"doctrine/doctrine-bundle": "~2.5",
		"doctrine/doctrine-migrations-bundle": "~3.2",
		"doctrine/orm": "~2.10",
		"lemuria-pbem/engine-fantasya": "~0.8.0",
		"lemuria-pbem/lemuria": "~0.8.0",
		"lemuria-pbem/model-fantasya": "~0.8.0",
		"phpdocumentor/reflection-docblock": "~5.3",
		"sensio/framework-extra-bundle": "~6.2",
		"symfony/apache-pack": "~1.0",
		"symfony/asset": "~5.4",
		"symfony/console": "~5.4",
		"symfony/dotenv": "~5.4",
		"symfony/expression-language": "~5.4",
		"symfony/flex": "~1.17",
		"symfony/form": "~5.4",
		"symfony/framework-bundle": "~5.4",
		"symfony/mailer": "~5.4",
		"symfony/monolog-bundle": "~3.7",
		"symfony/property-access": "~5.4",
		"symfony/property-info": "~5.4",
		"symfony/proxy-manager-bridge": "~5.4",
		"symfony/security-bundle": "~5.4",
		"symfony/serializer": "~5.4",
		"symfony/twig-bundle": "~5.4",
		"symfony/validator": "~5.4",
		"symfony/yaml": "~5.4",
		"twbs/bootstrap": "~4.6",
		"twig/extra-bundle": "~3.3",
		"twig/intl-extra": "~3.3"
	},
	"require-dev": {
		"phpunit/phpunit": "~9.5",
		"symfony/browser-kit": "~5.4",
		"symfony/css-selector": "~5.4",
		"symfony/debug-bundle": "~5.4",
		"symfony/maker-bundle": "~1.36",
		"symfony/phpunit-bridge": "~5.4",
		"symfony/stopwatch": "~5.4",
		"symfony/web-profiler-bundle": "~5.4"
	},
	"repositories": [
		{
			"type": "vcs",
			"url": "git@github.com:lemuria-pbem/lemuria.git"
		},
		{
			"type": "vcs",
			"url": "git@github.com:lemuria-pbem/model-fantasya.git"
		},
		{
			"type": "vcs",
			"url": "git@github.com:lemuria-pbem/engine-fantasya.git"
		}
	],

	"autoload": {
		"psr-4": {
			"App\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"App\\Tests\\": "tests/"
		}
	},

	"config": {
		"optimize-autoloader": true,
		"preferred-install": {
			"*": "dist"
		},
		"sort-packages": true
	},
	"conflict": {
		"symfony/symfony": "*"
	},
	"replace": {
		"paragonie/random_compat": "2.*",
		"symfony/polyfill-ctype": "*",
		"symfony/polyfill-iconv": "*",
		"symfony/polyfill-php80": "*"
	},
	"scripts": {
		"auto-scripts": {
			"cache:clear": "symfony-cmd",
			"assets:install %PUBLIC_DIR%": "symfony-cmd"
		},
		"create-directories": "bin/composer/create-directories.sh",
		"create-links": "bin/composer/create-links.sh",
		"post-install-cmd": [
			"@auto-scripts"
		],
		"post-update-cmd": [
			"@auto-scripts",
			"@create-directories",
			"@create-links"
		]
	},
	"scripts-descriptions": {
		"create-directories": "Create data directories.",
		"create-links": "Create Bootstrap CSS & JS links."
	},
	"extra": {
		"symfony": {
			"allow-contrib": false,
			"require": "~5.4"
		}
	}
}
