{
	"name": "fantasya-pbem/website",
	"description": "The Fantasya website.",
	"keywords": ["atlantis", "email", "eressea", "fantasya", "lemuria", "pbem", "symfony", "website"],
	"type": "project",
	"homepage": "https://www.fantasya-pbem.de",
	"support": {
		"chat": "https://web.libera.chat/?channels=#fantasya",
		"email": "info@fantasya-pbem.de",
		"forum": "https://forum.fantasya-pbem.de",
		"irc": "irc://libera.chat/fantasya",
		"issues": "https://github.com/fantasya-pbem/website/issues",
		"source": "https://github.com/fantasya-pbem/website",
		"wiki": "https://wiki.fantasya-pbem.de"
	},
	"authors": [
		{
			"name": "Sascha Ternes",
			"email": "info@fantasya-pbem.de"
		}
	],
	"funding": [
		{
			"type": "liberapay",
			"url": "https://liberapay.com/Fantasya"
		},
		{
			"type": "other",
			"url": "https://www.fantasya-pbem.de/donate"
		}
	],
	"license": "proprietary",
	"version": "2.4.0",
	"time": "2021-10-22",

	"require": {
		"php": "~8.0",
		"ext-ctype": "*",
		"ext-iconv": "*",
		"ext-pdo": "*",
		"components/jquery": "~3.6",
		"doctrine/annotations": "~1.13",
		"doctrine/doctrine-bundle": "^2.4",
		"doctrine/doctrine-migrations-bundle": "^3.2",
		"doctrine/orm": "^2.10",
		"lemuria-pbem/engine-fantasya": "~0.7.0",
		"lemuria-pbem/lemuria": "~0.7.0",
		"lemuria-pbem/model-fantasya": "~0.7.0",
		"phpdocumentor/reflection-docblock": "^5.3",
		"sensio/framework-extra-bundle": "~5.6",
		"symfony/apache-pack": "~1.0",
		"symfony/asset": "~5.3.0",
		"symfony/console": "~5.3.0",
		"symfony/dotenv": "~5.3.0",
		"symfony/expression-language": "~5.3.0",
		"symfony/flex": "~1.17",
		"symfony/form": "~5.3.0",
		"symfony/framework-bundle": "~5.3.0",
		"symfony/mailer": "~5.3.0",
		"symfony/monolog-bundle": "~3.7",
		"symfony/property-access": "~5.3.0",
		"symfony/property-info": "~5.3.0",
		"symfony/proxy-manager-bridge": "~5.3.0",
		"symfony/security-bundle": "~5.3.0",
		"symfony/serializer": "~5.3.0",
		"symfony/twig-bundle": "~5.3.0",
		"symfony/validator": "~5.3.0",
		"symfony/yaml": "~5.3.0",
		"twbs/bootstrap": "~4.6",
		"twig/extensions": "~1.5"
	},
	"require-dev": {
		"phpunit/phpunit": "^9.5",
		"symfony/browser-kit": "~5.3.0",
		"symfony/css-selector": "~5.3.0",
		"symfony/debug-bundle": "~5.3.0",
		"symfony/maker-bundle": "~1.34",
		"symfony/phpunit-bridge": "^5.3",
		"symfony/stopwatch": "~5.3.0",
		"symfony/web-profiler-bundle": "~5.3.0"
	},
	"repositories": [
		{
			"type": "vcs",
			"url": "git@github.com:lemuria-pbem/lemuria.git"
		},
		{
			"type": "vcs",
			"url": "git@github.com:lemuria-pbem/model-fantasya.git"
		},
		{
			"type": "vcs",
			"url": "git@github.com:lemuria-pbem/engine-fantasya.git"
		}
	],

	"autoload": {
		"psr-4": {
			"App\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"App\\Tests\\": "tests/"
		}
	},

	"config": {
		"preferred-install": {
			"*": "dist"
		},
		"sort-packages": true,
		"platform": {
			"php": "8.0.5"
		}
	},
	"conflict": {
		"symfony/symfony": "*"
	},
	"replace": {
		"paragonie/random_compat": "2.*",
		"symfony/polyfill-ctype": "*",
		"symfony/polyfill-iconv": "*",
		"symfony/polyfill-php80": "*",
		"symfony/polyfill-php74": "*",
		"symfony/polyfill-php73": "*",
		"symfony/polyfill-php72": "*"
	},
	"scripts": {
		"auto-scripts": {
			"cache:clear": "symfony-cmd",
			"assets:install %PUBLIC_DIR%": "symfony-cmd"
		},
		"create-directories": "bin/composer/create-directories.sh",
		"create-links": "bin/composer/create-links.sh",
		"post-install-cmd": [
			"@auto-scripts"
		],
		"post-update-cmd": [
			"@auto-scripts",
			"@create-directories",
			"@create-links"
		]
	},
	"scripts-descriptions": {
		"create-directories": "Create data directories.",
		"create-links": "Create Bootstrap CSS & JS links."
	},
	"extra": {
		"symfony": {
			"allow-contrib": false,
			"require": "~5.3.0"
		}
	}
}
