{
	"name": "fantasya-pbem/website",
	"description": "The Fantasya website.",
	"keywords": ["atlantis", "email", "eressea", "fantasya", "lemuria", "pbem", "pbm", "symfony", "website"],
	"license": "proprietary",
	"type": "project",
	"homepage": "https://www.fantasya-pbem.de",
	"support": {
		"chat": "https://web.libera.chat/?channels=#fantasya",
		"email": "info@fantasya-pbem.de",
		"forum": "https://forum.fantasya-pbem.de",
		"irc": "irc://libera.chat/fantasya",
		"issues": "https://github.com/fantasya-pbem/website/issues",
		"source": "https://github.com/fantasya-pbem/website",
		"wiki": "https://wiki.fantasya-pbem.de"
	},
	"funding": [
		{
			"type": "liberapay",
			"url": "https://liberapay.com/Fantasya"
		},
		{
			"type": "other",
			"url": "https://www.fantasya-pbem.de/donate"
		}
	],
	"authors": [
		{
			"name": "Sascha Ternes",
			"email": "info@fantasya-pbem.de"
		}
	],
	"version": "2.16.0",
	"time": "2023-05-31",

	"require": {
		"php": "~8.2",
		"ext-iconv": "~8.2",
		"ext-zip": "~1.21",
		"doctrine/annotations": "~2.0",
		"doctrine/doctrine-bundle": "~2.9",
		"doctrine/doctrine-migrations-bundle": "~3.2",
		"doctrine/orm": "~2.15",
		"lemuria-pbem/engine-fantasya": "~1.3.0",
		"lemuria-pbem/lemuria": "~1.3.0",
		"lemuria-pbem/model-fantasya": "~1.3.0",
		"symfony/apache-pack": "~1.0",
		"symfony/asset": "~6.3",
		"symfony/console": "~6.3",
		"symfony/dotenv": "~6.3",
		"symfony/expression-language": "~6.3",
		"symfony/flex": "~2.3",
		"symfony/form": "~6.3",
		"symfony/framework-bundle": "~6.3",
		"symfony/mailer": "~6.3",
		"symfony/monolog-bundle": "~3.8",
		"symfony/runtime": "~6.3",
		"symfony/security-bundle": "~6.3",
		"symfony/twig-bundle": "~6.3",
		"symfony/validator": "~6.3",
		"symfony/yaml": "~6.3",
		"twbs/bootstrap": "~5.3",
		"twig/extra-bundle": "~3.6",
		"twig/intl-extra": "~3.6"
	},
	"require-dev": {
		"phpunit/phpunit": "~10.0",
		"symfony/browser-kit": "~6.3",
		"symfony/css-selector": "~6.3",
		"symfony/debug-bundle": "~6.3",
		"symfony/maker-bundle": "~1.48",
		"symfony/phpunit-bridge": "~6.3",
		"symfony/stopwatch": "~6.3",
		"symfony/thanks": "~1.2",
		"symfony/web-profiler-bundle": "~6.3"
	},
	"repositories": [
		{
			"type": "vcs",
			"url": "git@github.com:lemuria-pbem/lemuria.git"
		},
		{
			"type": "vcs",
			"url": "git@github.com:lemuria-pbem/model-fantasya.git"
		},
		{
			"type": "vcs",
			"url": "git@github.com:lemuria-pbem/engine-fantasya.git"
		}
	],

	"autoload": {
		"psr-4": {
			"App\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"App\\Tests\\": "tests/"
		}
	},

	"config": {
		"preferred-install": {
			"*": "dist"
		},
		"sort-packages": true,
		"allow-plugins": {
			"symfony/flex": true,
			"symfony/runtime": true,
			"symfony/thanks": true
		}
	},
	"scripts": {
		"auto-scripts": {
			"cache:clear": "symfony-cmd",
			"assets:install %PUBLIC_DIR%": "symfony-cmd"
		},
		"create-directories": "bin/composer/create-directories.sh",
		"create-links": "bin/composer/create-links.sh",
		"post-install-cmd": [
			"@auto-scripts"
		],
		"post-update-cmd": [
			"@auto-scripts",
			"@create-directories",
			"@create-links"
		]
	},
	"scripts-descriptions": {
		"create-directories": "Create data directories.",
		"create-links": "Create Bootstrap CSS & JS links."
	},
	"extra": {
		"symfony": {
			"allow-contrib": true,
			"require": "~6.3.0"
		}
	}
}
